@callf = global i32 (i32)* @f, align 8

declare i32 @f(i32)

; Function Attrs: nounwind
define i32 @g(i32 %x) {
; CHECK-LABEL: g:
; CHECK:       # %bb.0: # %entry
; CHECK-NEXT:    make $r1 = callf
; CHECK-NEXT:    ;;
; CHECK-NEXT:    ld $r1 = 0[$r1]
; CHECK-NEXT:    ;;
; CHECK-NEXT:    igoto $r1
; CHECK-NEXT:    ;;
entry:
  %0 = load i32 (i32)*, i32 (i32)** @callf, align 8
  %call = tail call i32 %0(i32 %x)
  ret i32 %call

}
