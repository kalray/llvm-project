target triple = "kvx-kalray-cos"

define <2 x float> @t(<2 x float> %a, <2 x float> %b) {
; CHECK-LABEL: t:
; CHECK:       # %bb.0: # %entry
; CHECK-NEXT:    fmulwp $r0 = $r0, $r1
; CHECK-NEXT:    ret
; CHECK-NEXT:    ;;
entry:
  %mul = fmul <2 x float> %a, %b
  ret <2 x float> %mul
}
