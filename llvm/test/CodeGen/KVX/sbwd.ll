; NOTE: Assertions have been autogenerated by utils/update_llc_test_checks.py
; RUN: llc -o - %s | FileCheck %s
target triple = "kvx-kalray-cos"


define i64 @SBFWD(i64 %0, i32 %1) {
; CHECK-LABEL: SBFWD:
; CHECK:       # %bb.0:
; CHECK-NEXT:    sbfwd $r0 = $r1, $r0
; CHECK-NEXT:    ret
; CHECK-NEXT:    ;;
  %3 = sext i32 %1 to i64
  %4 = sub nsw i64 %0, %3
  ret i64 %4
}

define i64 @SBFUWD(i64 %0, i32 %1) {
; CHECK-LABEL: SBFUWD:
; CHECK:       # %bb.0:
; CHECK-NEXT:    zxwd $r1 = $r1
; CHECK-NEXT:    ;;
; CHECK-NEXT:    sbfd $r0 = $r1, $r0
; CHECK-NEXT:    ret
; CHECK-NEXT:    ;;
  %3 = zext i32 %1 to i64
  %4 = sub nsw i64 %0, %3
  ret i64 %4
}
